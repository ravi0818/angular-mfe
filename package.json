{
  "name": "angular-mfe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build:common": "ng build common",
    "build:host-app": "ng build host-app",
    "build:todos-app": "ng build todos-app",
    "build:shopping-app": "ng build shopping-app",
    "start:host-app": "ng serve host-app",
    "start:todos-app": "ng serve todos-app",
    "start:shopping-app": "ng serve shopping-app",
    "build-prod:host-app": "ng build host-app --configuration production",
    "build-prod:todos-app": "ng build todos-app --configuration production",
    "build-prod:shopping-app": "ng build shopping-app --configuration production",
    "start-prod:host-app": "ng serve host-app --configuration production --host 0.0.0.0 --disable-host-check",
    "start-prod:todos-app": "ng serve todos-app --configuration production --host 0.0.0.0 --disable-host-check",
    "start-prod:shopping-app": "ng serve shopping-app --configuration production --host 0.0.0.0 --disable-host-check",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers=ChromeHeadless",
    "test:coverage": "ng test --no-watch --code-coverage --browsers=ChromeHeadless",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "dev:ssr": "ng run host-app:serve-ssr",
    "serve:ssr": "node dist/host-app/server/main.js",
    "build:ssr": "ng build && ng run host-app:server",
    "prerender": "ng run host-app:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^19.0.1",
    "@angular/animations": "^19.1.5",
    "@angular/cdk": "^19.1.3",
    "@angular/common": "^19.1.5",
    "@angular/compiler": "^19.1.5",
    "@angular/core": "^19.1.5",
    "@angular/forms": "^19.1.5",
    "@angular/material": "^19.1.3",
    "@angular/platform-browser": "^19.1.5",
    "@angular/platform-browser-dynamic": "^19.1.5",
    "@angular/platform-server": "^19.1.5",
    "@angular/router": "^19.1.5",
    "@angular/ssr": "^19.1.6",
    "@ngrx/store": "^19.0.0",
    "express": "^4.18.2",
    "jwt-decode": "^4.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.6",
    "@angular/build": "^19.1.6",
    "@angular/cli": "^19.1.6",
    "@angular/compiler-cli": "^19.1.5",
    "@ngrx/schematics": "^19.0.1",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "autoprefixer": "^10.4.20",
    "browser-sync": "^3.0.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.1.0",
    "ngx-build-plus": "^19.0.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2"
  }
}